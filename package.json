{
  "name": "x-quote-to-warpcast",
  "description": "This extension helps you quote X posts on warpcast by providing a button to all posts on X.",
  "private": true,
  "version": "1.0.2",
  "type": "module",
  "scripts": {
    "inject": "tsc --downlevelIteration --outFile src/client/inject.js src/client/inject.ts",
    "post-build": "bun run ./scripts/post-build.ts",
    "release": "bun run ./scripts/version-release.ts",
    "dev": "vite",
    "build": "bun run inject && vite build && bun run post-build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "pretty": "prettier --write .",
    "postversion": "git push",
    "create-release": "tsx ./scripts/create-release.ts",
    "rebuild": "bun run build && bun run ./scripts/create-release.ts rebuild",
    "pub": "bun run build && bun run release && bun run ./scripts/create-release.ts",
    "only-pub": "bun run build && bun run ./scripts/create-release.ts"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@crxjs/vite-plugin": "^2.0.0-beta.23",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tsconfig/svelte": "^5.0.2",
    "@types/chrome": "^0.0.263",
    "@types/node": "^20.11.28",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.2.5",
    "sass": "^1.58.0",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.6",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.6.2",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.6"
  }
}
